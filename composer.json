{
    "name": "happytodev/blogr",
    "description": "Blogr is a FilamentPHP plugin that adds a powerful blog system to your Laravel application.",
    "keywords": [
        "happytodev",
        "laravel",
        "filamentphp",
        "blogr"
    ],
    "version": "0.2.2",
    "homepage": "https://github.com/happytodev/blogr",
    "support": {
        "issues": "https://github.com/happytodev/blogr/issues",
        "source": "https://github.com/happytodev/blogr"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Blanc",
            "email": "happytodev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Happytodev\\Blogr\\": "src/",
            "Happytodev\\Blogr\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Happytodev\\Blogr\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Happytodev\\Blogr\\BlogrServiceProvider"
            ],
            "aliases": {
                "Blogr": "Happytodev\\Blogr\\Facades\\Blogr"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}